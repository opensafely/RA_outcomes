
. * loop through both cohorts
. foreach year in 2018-03-01 2020-03-23 {
  2.     import delimited using "./output/input_monitor_bl_`year'.csv", clear
  3.     * Inital check of frequency of DMARD prescribing and blood tests
.     di "number of missing age"
  4.     count if age==. 
  5.     di "number under age 18"
  6.     count if age<18 
  7.     di "number older than 110"
  8.     count if age>110 & age!=.
  9.     sum age, d
 10.     di "number with missing gender"
 11.     count if sex==""
 12.     tab sex
 13.     sum household, d
 14.     tab care_home
 15.     * Checking rheumatic disease
.     di "number with no RA, psoriasis or psoriatic arthritis"
 16.     count if has_ra_code==0 & has_psoriasis_code==0 & has_psoriatic_arthri
> tis_code==0
 17.     egen dis_tot = rowtotal(has_ra_code has_psoriatic_arthritis_code has_p
> soriasis_code)
 18.     tab dis_tot
 19.     * Frequency of and time since first rheumatic disease code
.     foreach var in ra psoriasis psoriatic_arthritis {
 20.         tab has_`var'_code
 21.         gen time_`var' = 2018 - first_`var'_code
 22.         sum time_`var', d
 23.     }
 24.     
.     * Checking how frequently each type of drug is prescribed & bloods are mo
> ntiored
.     foreach var in metho leflu aza fbc lft creatinine {
 25.         sum `var'_count, d
 26.         gen `var'_monitored = (`var'_count>=3 & `var'_count!=.)
 27.     }
 28.     gen bloods_monitored=(fbc_count>=3 & lft_count>=3 & creatinine_count>=
> 3)
 29.     tab bloods_monitored, m
 30. 
.     di "People with 4+ prescriptions and rheum code"
 31.     tab dmard_rheum_prior, m
 32.     tab dmard_rheum_prior bloods_monitored
 33. 
.     gen dmard_monitored_prior = (dmard_rheum_prior==1 & bloods_monitored==1)
 34. 
.     * Format variables
.     *re-order ethnicity
.     gen eth5=1 if ethnicity==1
 35.     replace eth5=2 if ethnicity==3
 36.     replace eth5=3 if ethnicity==4
 37.     replace eth5=4 if ethnicity==2
 38.     replace eth5=5 if ethnicity==5
 39.     replace eth5=. if ethnicity==.
 40. 
.     label define eth5                   1 "White"                            
>            ///
>                                 2 "South Asian"                         ///  
>                                            
>                                 3 "Black"                                    
>    ///
>                                 4 "Mixed"                                    
>    ///
>                                 5 "Other"                                    
>    
 41.                         
. 
.     label values eth5 eth5
 42.     safetab eth5, m
 43. 
.     * formatting gender
.     gen male=(sex=="M")
 44.     replace male = 0 if sex == "F"
 45.     label define male 0"Female" 1"Male"
 46.     label values male male
 47.     safetab male, miss
 48. 
.     * Make region numeric
.     generate region2=.
 49.     replace region2=0 if region=="East"
 50.     replace region2=1 if region=="East Midlands"
 51.     replace region2=2 if region=="London"
 52.     replace region2=3 if region=="North East"
 53.     replace region2=4 if region=="North West"
 54.     replace region2=5 if region=="South East"
 55.     replace region2=6 if region=="South West"
 56.     replace region2=7 if region=="West Midlands"
 57.     replace region2=8 if region=="Yorkshire and The Humber"
 58.     drop region
 59.     rename region2 region
 60.     label var region "region of England"
 61.     label define region 0 "East" 1 "East Midlands"  2 "London" 3 "North Ea
> st" 4 "North West" 5 "South East" 6 "South West" 7 "West Midlands" 8 "Yorkshi
> re and The Humber"
 62.     label values region region
 63.     safetab region, miss
 64. 
.     *create a 4 category rural urban variable 
.     generate urban_rural_5=.
 65.     la var urban_rural_5 "Rural Urban in five categories"
 66.     replace urban_rural_5=1 if urban_rural==1
 67.     replace urban_rural_5=2 if urban_rural==2
 68.     replace urban_rural_5=3 if urban_rural==3|urban_rural==4
 69.     replace urban_rural_5=4 if urban_rural==5|urban_rural==6
 70.     replace urban_rural_5=5 if urban_rural==7|urban_rural==8
 71.     label define urban_rural_5 1 "Urban major conurbation" 2 "Urban minor 
> conurbation" 3 "Urban city and town" 4 "Rural town and fringe" 5 "Rural villa
> ge and dispersed"
 72.     label values urban_rural_5 urban_rural_5
 73.     safetab urban_rural_5, miss
 74. 
.     *generate a binary rural urban (with missing assigned to urban)
.     generate urban_rural_bin=.
 75.     replace urban_rural_bin=1 if urban_rural<=4|urban_rural==.
 76.     replace urban_rural_bin=0 if urban_rural>4 & urban_rural!=.
 77.     label define urban_rural_bin 0 "Rural" 1 "Urban"
 78.     label values urban_rural_bin urban_rural_bin
 79.     safetab urban_rural_bin urban_rural, miss
 80.     label var urban_rural_bin "Rural-Urban"
 81. 
.     * Define age categories
.     * Create age categories
.     egen age_cat = cut(age), at(18, 40, 60, 80, 120) icodes
 82.     label define age 0 "18 - 40 years" 1 "41 - 60 years" 2 "61 - 80 years"
>  3 ">80 years"
 83.     label values age_cat age
 84.     safetab age_cat, miss
 85. 
.     * Flag if in a care-home
.     gen care_home=care_home_type!="PR"
 86. 
.     * Tabulate
.     table1_mc, vars(age_cat cate \ male cate \ region cate \ urban_rural_5 ca
> te \ comorbidity cate \ imd cate \ care_home cate) by(dmard_monitored_prior)
 87.     *export delimited using ./output/tables/op_appt_yrs.csv
. }
